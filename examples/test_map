a = { 
	1 : 1+2+3+4+5,
	"str1" : "1+2+3+4+5",
	0x123 : 0b1010101,
};

expect(a["str1"] == "1+2+3+4+5", a["str1"]);
expect(a[1] == 15, "a[1] == ", a[1]);
expect(a[0x123] == 0b1010101, "a[0x123] == ", a[0x123]);


a = { };

n = 10;
i = 0;
while i < n {
	a[i] = i;
	i = i + 1;
}

i = 0;
while i < n {
	expect(a[i] == i, a[i], " == ", i);
	i = i + 1;
}

nested = {
	"0" : { 
		"0" : "A", 
		"1" : "B"
	},
	"1" : { 
		"0" : "C", 
		"1" : "D"
	},	
};

temp = nested["0"];
expect(temp["0"] == "A", "nested[\"0\"][\"0\"] ==", temp);
expect(temp["1"] == "B", "nested[\"0\"][\"1\"] ==", temp);
temp = nested["1"];
expect(temp["0"] == "C", "nested[\"1\"][\"0\"] ==", temp);
expect(temp["1"] == "D", "nested[\"1\"][\"1\"] ==", temp);