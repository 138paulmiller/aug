a0 = [ 1, 10, 100 ];
a1 = [ 1000, 10000];
a = append(a0, a1);
b = sum(a);
expect(b == 11111, "sum(append(", a0, ",", a1, ")) = ", b);
print(a0, a1);
print(a);

a = [ 0,1,2,3,4,5];
b = sum(a);
expect(b == 15, "sum(", a, ") = ", b);

a = [ 0, 1, [ 2, 3, 4 ], 5, [6]];
b = sum(a);
expect(b == 21, "sum(", a, ") = ", b);


a = [0, 1,2,3,4,5];

e = a[1];
expect(e == 1, "a[1] = ", e);

e = a[2];
expect(e == 2, "a[2] = ", e);

e = a[ 1 + 1];
expect(e == 2, "a[1+1] = ", e);

i = 5;
e = a[i];
expect(e == 5, "a[i] = ", e);