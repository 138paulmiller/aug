func fib_recursive(a) {
    if a == 0 { 
        return 0; 
    }
    else if a == 1 { 
        return 1; 
    }
    return fib_recursive(a-1) + fib_recursive(a-2);
}

a = fib_recursive(20);
expect(a == 6765, "fib_recursive(50) = ", a);

a = fib_recursive(30);
expect(a == 832040, "fib_recursive(50) = ", a);