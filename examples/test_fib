func fibonacci(n) {
    var a = 0;
    var b = 1;
    var c = 0;
    var sum = 0;
    var count = n;

    while count > 0 {
        count = count - 1; 
        a = b ;
        b = sum; 
        sum = a + b;
    }
    return sum;
}


a = fibonacci(20);
expect(a == 6765, "fibonacci(20) = ", a);

a = fibonacci(30);
expect(a == 832040, "fibonacci(30) = ", a);