func fibonacci(n) {
    #var a = 0;
    #var b = 1;
    #var c = 0;
    #var sum = 0;
    var count = n;

    while count > 0 {
        count = count - 1; 
        #print(count);
        #a = b ;
        #b = sum; 
        #sum = a + b;
    }
    #return sum;
}

var a = fibonacci(2000000);
print(a);