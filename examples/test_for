var a = [1, 10, 100, 1000, 10000];
var total1 = 0;
for x in a { 
    total1 = total1 + x; 
}
expect( total1 == 11111);
expect( sum_array(a) == total1);

var total2 = 0;
for x in [1, 10, 100, 1000, 10000] { 
    total2 = total2 + x; 
}
expect( total2 == 11111);
expect( sum_array([1, 10, 100, 1000, 10000] ) == total2);


func sum_array(arr){
    var total = 0;
    for x in arr{ 
        total += x; 
    }
    return total;  
}