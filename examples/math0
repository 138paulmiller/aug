a = 1;
b = 2.0;
c = a/b;
d = 1.0/2.0;

print("a=", a);
print("b=", b);
print("c=", c);
print("d=", d);
print("1.0 + 2.0 * 3 / 4.0 - 5=", 1.0 + 2.0 * 3 / 4.0 - 5);

a = 1;
b = 10;
c = 100;
d = 1000;

print("a=", a);
print("b=", b);
print("c=", c);
print("d=", d);
