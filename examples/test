print("1 + 2 \t", 1 + 2, " == 3  ");
print("1 - 2 \t", 1 - 2, " == -1 ");
print("1 * 2 \t", 1 * 2, " == 2  ");
print("1 / 2 \t", 1 / 2, " == 0.5");
print("1 + -2 \t", 1 + -2, " == -1  ");
print("1 - -2 \t", 1 - -2, " == 3   ");
print("1 * -2 \t", 1 * -2, " == -2  ");
print("1 / -2 \t", 1 / -2, " == -0.5");
print("1 < 2  \t", 1 < 2 , " == true" );
print("1 <= 2 \t", 1 <= 2, " == true" );
print("1 > 2  \t", 1 > 2 , " == false");
print("1 >= 2 \t", 1 >= 2, " == false");
print("1 == 1 \t", 1 == 1, " == true");
print("1 == 2 \t", 1 == 2, " == false");
print("1 == 2 \t", 1 == 2, " == false");
print("1 == 1 \t", 1 == 1, " == true");
print("1 != 1 \t", 1 != 1, " == false");
print("1 != 2 \t", 1 != 2, " == true");

print("1.0 + 2.0 \t", 1.0 + 2.0, " == 3.0  ");
print("1.0 - 2.0 \t", 1.0 - 2.0, " == -1.0 ");
print("1.0 * 2.0 \t", 1.0 * 2.0, " == 2.0  ");
print("1.0 / 2.0 \t", 1.0 / 2.0, " == 0.5  ");
print("1.0 < 2.0  \t", 1.0 < 2.0 , " == true" );
print("1.0 <= 2.0 \t", 1.0 <= 2.0, " == true" );
print("1.0 > 2.0  \t", 1.0 > 2.0 , " == false");
print("1.0 >= 2.0 \t", 1.0 >= 2.0, " == false");

print("true == false \t", true == false, " == false");
print("true != false \t", true != false, " == true");

print("---- BEGIN ---- ");

running = true;
a = 0;
while running {
    if a == 1300000 {
        print("Found 13"); 
        running = false;
    }
    a = a + 1;
}

print("---- END ---- "); 

