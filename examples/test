a = 1;

func fibonacci(n) {
    a = 0;
    b = 1;
    c = 0;
    sum = 0;
    count = n;

    while count > 0 {
        count = count - 1; 
        a = b ;
        b = sum; 
        sum = a + b;
    }
    return sum;
}

expect(a == 1, "a == ", a);


a = fibonacci(30);
expect(a == 832040, "fibonacci(30) = ", a);

a = fibonacci(20);
expect(a == 6765, "fibonacci(20) = ", a);

func fibonacci_rec(a) {
    if a == 0 { 
        return 0; 
    }
    else if a == 1 { 
        return 1; 
    }
    return fibonacci_rec(a-1) + fibonacci_rec(a-2);
}

a = fibonacci_rec(20);
expect(a == 6765, "fibonacci_rec(20) = ", a);

a = fibonacci_rec(30);
expect(a == 832040, "fibonacci_rec(30) = ", a);


