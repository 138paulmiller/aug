func fizzbuzz(i)
{
    var a = i % 3 == 0;
    var b = i % 5 == 0;
    if a and b
    {
        var c  = a and b;
        expect(c, "Fizzbuzz");
    }
    else if a
    {
        var c  = a and !b;
        expect(c, "Fizz");
    }
    else if b
    {
        var c  = !a and b;
        expect(c, "Buzz");
    }
}

func fibonacci_fizzbuzz(n) {
    var a = 0;
    var b = 1;
    var c = 0;
    var sum = 0;
    var count = 1;
    while count <= n {
        count = count + 1; 
        var d = count;
        fizzbuzz(d);
        a = b;
        b = sum; 
        sum = a + b;
    }
    return sum;
}

a = fibonacci_fizzbuzz(30);
expect(a == 832040, "fibonacci_fizzbuzz(30) = ", a);